{
    "name": "Apache Celix Dev Container",
    "build": {
        "dockerfile": "Containerfile",
        "target": "develop"
    },
     "runArgs" : [
        "--userns=keep-id"
    ],
    "onCreateCommand": "sh .devcontainer/setup-conan.sh",
    "postCreateCommand": "sh .devcontainer/build-conan.sh"
}
